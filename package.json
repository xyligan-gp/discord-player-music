    "url": "https://github.com/xyligan-gp/discord-player-music/issues"
  },
  "homepage": "https://dpm.js.org",
  "keywords": [
    "discord",
    "discord-bot",
    "sidcord-music",
    "discord.js",
    "discord-player",
    "discord-player-music",
    "ytdl-core",
    "yt-search",
    "xyligan"
  ],
  "dependencies": {
    "@discordjs/opus": "^0.8.0",
    "@discordjs/voice": "^0.11.0",
