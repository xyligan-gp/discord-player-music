{
	"name": "discord-player-music",
	"version": "2.0.0-dev",
	"description": "A powerful music module for your Discord bot with lots of features",
	"main": "index.js",
	"scripts": {
		"build": "rimraf dist && tsc -p tsconfig.json",
		"build:old": "tsc -p tsconfig.json",
		"build:docs": "docgen -s *.ts src/*.ts -o docs/index.json -c docs/index.yml -g -S 1 -j .jsconfig.json",
		"pub": "npm publish",
		"pub:dev": "npm publish --tag dev"
	},
	"bugs": {
		"url": "https://github.com/xyligan-gp/discord-player-music/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/xyligan-gp/discord-player-music.git"
	},
	"homepage": "https://dpm.js.org",
	"keywords": [
		"discord",
		"discord-bot",
		"sidcord-music",
		"discord.js",
		"discord-player",
		"discord-player-music",
		"ytdl-core",
		"yt-search",
		"xyligan"
	],
	"author": "xyligan",
	"license": "MIT",
	"engines": {
		"node": ">=16.9.0"
	},
	"types": "./types/index.d.ts",
	"dependencies": {
		"@discordjs/voice": "^0.16.0",
		"discord.js": "^14.11.0"
	},
	"devDependencies": {
		"@babel/core": "^7.22.5",
		"@babel/plugin-proposal-class-properties": "^7.18.6",
		"@babel/plugin-proposal-object-rest-spread": "^7.20.7",
		"@babel/preset-env": "^7.22.5",
		"@babel/preset-typescript": "^7.22.5",
		"@distube/docgen": "github:distubejs/docgen",
		"@types/events": "^3.0.0",
		"jsdoc-babel": "^0.5.0",
		"rimraf": "^5.0.1",
		"typescript": "^5.1.3"
	}
}
